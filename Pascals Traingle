class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> ans;
        
        for(int i(1);i <= numRows;i++){
            vector<int> x; int s(1);
            x.push_back(1);
            for(int j(1);j < i;j++){
                s *= (i - j); s /= j; x.push_back(s);
            }
            ans.push_back(x);
        }

        return ans;
    }
};
